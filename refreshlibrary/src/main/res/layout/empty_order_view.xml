<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:background="@color/color_background_f2f4f5"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView4_order"
        android:layout_width="@dimen/icon_image_size"
        android:layout_height="@dimen/icon_image_size"
        android:layout_marginBottom="@dimen/icon_image_bottom"
        android:layout_marginTop="@dimen/icon_image_top"
        android:background="@mipmap/icon_no_order" />
    <TextView
        android:id="@+id/session_order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/icon_text_maringtop"
        android:text="您暂时还没有订单"
        android:textColor="@color/color_text_666666"
        android:textSize="@dimen/textSize_title_14" />
    <TextView
        android:id="@+id/textView3_order"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="empty"
        android:textColor="@color/color_00b459"
        android:textSize="@dimen/textSize_title_14"
        android:visibility="gone"
         />
</LinearLayout>
